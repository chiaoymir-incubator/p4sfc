{
  "target": "bmv2",
  "p4info": "build/sfc_proxy.p4.p4info.txt",
  "bmv2_json": "build/sfc_proxy.json",
  "table_entries": [
    {
      "table": "MyIngress.ipv4_lpm",
      "default_action": true,
      "action_name": "MyIngress.drop",
      "action_params": { }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dstAddr": ["10.0.2.2", 32]
      },
      "action_name": "MyIngress.enable_sfc",
      "action_params": {
        "max_size": 4,
        "qos": 1,
        "dst_id": 11
      }
    },
    {
      "table": "MyIngress.sfc_forward_exact",
      "match": {
        "hdr.sfc_header.dst_id": 11
      },
      "action_name": "MyIngress.forward_sfc",
      "action_params": {
        "port": 2
      }
    },
    {
      "table": "MyIngress.sfc_disable_exact",
      "match": {
        "hdr.sfc_header.dst_id": 12
      },
      "action_name": "MyIngress.forward_sfc",
      "action_params": {
        "dstAddr": "08:00:00:00:01:11",
        "port": 1
      }
    }
  ]
}
